                <size>
                    <width>20</width>
                    <height>21</height>
                </size>
            </property>
        </spacer>
    </vbox>
</widget>
<connections>
    <connection>
        <sender>m_audioPluginComboBox</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>page2layout</receiver>
        <slot>pluginChanged(const QString&amp;)</slot>
    </connection>
</connections>
<slots>
    <slot>pluginChanged(const QString&amp;)</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
</UI>
